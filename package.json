{
  "name": "route-sort",
  "version": "1.0.0",
  "repository": "lukeed/route-sort",
  "description": "A tiny (200B) utility to sort route patterns by specificity",
  "module": "dist/rsort.mjs",
  "unpkg": "dist/rsort.min.js",
  "main": "dist/rsort.js",
  "types": "rsort.d.ts",
  "umd:name": "rsort",
  "license": "MIT",
  "author": {
    "name": "Luke Edwards",
    "email": "luke.edwards05@gmail.com",
    "url": "https://lukeed.com"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "build": "bundt",
    "pretest": "npm run build",
    "test": "tape -r esm test/*.js | tap-spec"
  },
  "keywords": [
    "exact",
    "route",
    "pattern",
    "specificity",
    "sort"
  ],
  "devDependencies": {
    "bundt": "0.4.0",
    "esm": "3.2.25",
    "tap-spec": "5.0.0",
    "tape": "4.11.0"
  }
}
